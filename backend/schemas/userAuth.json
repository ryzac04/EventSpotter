{
    "schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/userNew.json",
    "type": "object",
    "default": {},
    "title": "User Authentication",
    "properties": {
        "id": {
            "type": "integer"
        },
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "password": {
            "type": "string",
            "minLength": 6
        }
    },
    "additionalProperties": false,
    "required": [
        "username",
        "password"
    ]
}