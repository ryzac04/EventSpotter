{
    "schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/userNew.json",
    "type": "object",
    "default": {},
    "title": "User Registration",
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "password": {
            "type": "string",
            "minLength": 6
        },
        "email": {
            "type": "string",
            "format": "email",
            "maxLength": 100
        },
        "is_admin": {
            "type": "boolean",
            "default": false
        }
    },
    "additionalProperties": false,
    "required": [
        "username",
        "password",
        "email"
    ]
}